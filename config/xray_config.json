{
  "log": {
    "access": "./access.log",
    "dnsLog": false,
    "error": "./error.log",
    "loglevel": "info",
    "maskAddress": ""
  },
  "routing": {
    "rules": [
      {
        "ip": [
          "geoip:private"
        ],
        "outboundTag": "BLOCK",
        "type": "field"
      },
      {
        "type": "field",
        "domain": [
          "geosite:category-ads-all"
        ],
        "outboundTag": "block"
      },
      {
        "type": "field",
        "domain": [
          "geosite:google"
        ],
        "outboundTag": "IPv4"
      },
      {
        "type": "field",
        "domain": [
          "domain:gemini.google.com"
        ],
        "outboundTag": "IPv4"
      },
      {
        "type": "field",
        "domain": [
          "geosite:openai"
        ],
        "outboundTag": "IPv4"
      },
      {
        "type": "field",
        "domain": [
          "geosite:intel",
          "geosite:category-gov-ru"
        ],
        "outboundTag": "warp"
      },
      {
        "type": "field",
        "ip": [
          "geoip:ru"
        ],
        "outboundTag": "warp"
      },
      {
        "type": "field",
        "domain": [
          "regexp:.*\\.xn--$",
          "regexp:.*\\.ru$",
          "regexp:.*\\.su$"
        ],
        "outboundTag": "warp"
      }
    ]
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": 7443,
      "protocol": "vless",
      "settings": {
        "clients": [],
        "decryption": "none",
        "fallbacks": []
      },
      "streamSettings": {
        "network": "tcp",
        "realitySettings": {
          "dest": "127.0.0.1:36077",
          "maxClient": "",
          "maxTimediff": 0,
          "minClient": "",
          "privateKey": "TEMP_PRIVATEKEY0",
          "serverNames": [
            "TEMP_DOMAIN"
          ],
          "shortIds": [
            ""
          ],
          "show": false,
          "xver": 2
        },
        "security": "reality",
        "tcpSettings": {
          "acceptProxyProtocol": true,
          "header": {
            "type": "none"
          }
        }
      },
      "tag": "Steal",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls",
          "quic",
          "fakedns"
        ],
        "metadataOnly": false,
        "routeOnly": false
      }
    },
    {
      "listen": "127.0.0.1",
      "port": 9443,
      "protocol": "vless",
      "settings": {
        "clients": [],
        "decryption": "none",
        "fallbacks": [
          {
            "alpn": "",
            "dest": "127.0.0.1:9090",
            "name": "",
            "path": "",
            "xver": 0
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tcpSettings": {
          "acceptProxyProtocol": true,
          "header": {
            "type": "none"
          }
        },
        "tlsSettings": {
          "alpn": [
            "http/1.1"
          ],
          "certificates": [
            {
              "buildChain": false,
              "certificateFile": "/var/lib/marzban/certs/fullchain.pem",
              "keyFile": "/var/lib/marzban/certs/key.pem",
              "ocspStapling": 3600,
              "oneTimeLoading": false,
              "usage": "encipherment"
            }
          ],
          "cipherSuites": "",
          "disableSystemRoot": false,
          "enableSessionResumption": false,
          "maxVersion": "1.3",
          "minVersion": "1.3",
          "rejectUnknownSni": false,
          "serverName": "www.TEMP_DOMAIN"
        }
      },
      "tag": "XTLS",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ],
        "metadataOnly": false,
        "routeOnly": false
      },
      "allocate": {
        "strategy": "always",
        "refresh": 5,
        "concurrency": 3
      }
    }
  ],
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv4",
        "redirect": "",
        "noises": []
      }
    },
    {
      "tag": "block",
      "protocol": "blackhole",
      "settings": {}
    },
    {
      "tag": "IPv4",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIPv4",
        "redirect": "",
        "noises": []
      }
    },
    {
      "tag": "warp",
      "protocol": "socks",
      "settings": {
        "servers": [
          {
            "address": "127.0.0.1",
            "port": 40000,
            "users": []
          }
        ]
      }
    }
  ]
}
